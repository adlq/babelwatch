msgid "<html>\n<head>\n<meta http-equiv=\"Content-Type\" content=\"text/html; charset=utf-8\" />\n\n</head>\n<body bgcolor=\"#FFFFFF\">\n<table cellpadding=\"0\" cellspacing=\"0\" align=\"center\" width=\"600\">\n<tr>\n<td bgcolor=\"#052647\"><font size=\"3\" face=\"Verdana, Arial, Helvetica, sans-serif\" color=\"#FFFFFF\"><strong> &nbsp;New enrolment for confirmation<br />&nbsp; </strong></font></td>\n</tr>\n<tr>\n<td ><font  face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#052647\"><strong>\n</strong></font><br />\n<font face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#000000\">&nbsp;      </font>\n<table width=\"100%\" border=\"1\" cellspacing=\"0\" cellpadding=\"5\" bordercolor=\"#052647\">\n<tr>\n<td><font face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#000000\" style=\"font-size: 11px;\">\n<p>Hello,</p>\n<p align=\"justify\">A learner has been enrolled for an ESSEC course on the following platform:<br>\n{url}<br>\n<br />As administrator, you must confirm this enrolment before he/she can begin training.</p>\n<p align=\"justify\">Kind regards, <br><em>The training team</em><br />ESSEC Program</p></p>\n\n</font></td>\n</tr>\n</table>\n<br />\n</td>\n</tr>\n</table></td>\n</tr>\n</table>\n</body>\n</html>"
msgstr ""

msgid "<html>\n<head>\n<meta http-equiv=\"Content-Type\" content=\"text/html; charset=utf-8\" />\n\n</head>\n<body bgcolor=\"#FFFFFF\">\n<table cellpadding=\"0\" cellspacing=\"0\" align=\"center\" width=\"600\">\n<tr>\n<td bgcolor=\"#052647\"><font size=\"3\" face=\"Verdana, Arial, Helvetica, sans-serif\" color=\"#FFFFFF\"><strong> &nbsp;New learner to be allocated <br />&nbsp; </strong></font></td>\n</tr>\n<tr>\n<td ><font  face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#052647\"><strong>\n</strong></font><br />\n<font face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#000000\">&nbsp;      </font>\n<table width=\"100%\" border=\"1\" cellspacing=\"0\" cellpadding=\"5\" bordercolor=\"#052647\">\n<tr>\n<td><font face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#000000\" style=\"font-size: 11px;\">\n<p>Hello {teacher_name},</p>\n<p align=\"justify\">{candidate_name} has just enrolled as a learner on the ESSEC course <strong>{training_title}</strong>.\n<br>\n<br />If you wish to be his/her trainer, please log on to your trainer page and complete the learner allocation process: <strong>{url} </strong></p>\n<p align=\"justify\">Kind regards, <br><em>The training team</em><br />ESSEC Program</p></p>\n\n</font></td>\n</tr>\n</table>\n\n</body>\n</html>"
msgstr ""

msgid "<html>\r\n<head>\r\n<meta http-equiv=\"Content-Type\" content=\"text/html; charset=utf-8\" />\r\n\r\n</head>\r\n<body bgcolor=\"#FFFFFF\">\r\n<table cellpadding=\"0\" cellspacing=\"0\" align=\"center\" width=\"600\">\r\n<tr>\r\n<td bgcolor=\"#052647\"><font size=\"3\" face=\"Verdana, Arial, Helvetica, sans-serif\" color=\"#FFFFFF\"><strong> &nbsp;{subject} <br />&nbsp; </strong></font></td>\r\n</tr>\r\n<tr>\r\n<td ><font  face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#052647\"><strong>\r\n</strong></font><br />\r\n<font face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#000000\">&nbsp;      </font>\r\n<table width=\"100%\" border=\"1\" cellspacing=\"0\" cellpadding=\"5\" bordercolor=\"#052647\">\r\n<tr>\r\n<td><font face=\"Verdana, Arial, Helvetica, sans-serif\" size=\"2\" color=\"#000000\" style=\"font-size: 11px;\">\r\n<p align=\"justify\">A new contribution has been added to the training course <strong>{training_title}</strong>.\r\n<br></p>\r\n<p style=\"text-align: center; font-size: 16px;\"><a style=\"padding: 5px; background-color: #E3E9FF;  border: 1px solid #BBCCFF; color:black; font-weight: bold;\" href=\"{url}\">Manage the contribution</a></p>\r\n<p></p>\r\n</font></td>\r\n</tr>\r\n</table>\r\n\r\n</body>\r\n</html>"
msgstr ""

